@model IEnumerable<TaskLog2ndGen.Models.Employee>

<p></p>

<table class="table table-bordered table-responsive">
    <thead>
        <tr>
            <th colspan="2">Admin Report</th>
            <th colspan="3"></th>
            <th colspan="2" class="text-right">Created By: @ViewBag.createdBy</th>
        </tr>
        <tr>
            <th colspan="7">From @string.Format("{0:MM/dd/yyyy}", ViewBag.startDate) - @string.Format("{0:MM/dd/yyyy}", ViewBag.endDate)</th>
        </tr>
        <tr>
            <th>
                Date
            </th>
            <th>
                Date Updated
            </th>
            <th>
                Employee Id
            </th>
            <th>
                First Name
            </th>
            <th>
                Last Name
            </th>
            <th>
                E-mail
            </th>
            <th>
                Phone
            </th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td class="text-center">@string.Format("{0:MM/dd/yyyy}", DateTime.Now)</td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td></td>
                <td class="text-center">@string.Format("{0:MM/dd/yyyy}", item.lastChanged)k</td>
                <td class="text-center">@item.employeeId</td>
                <td class="text-center">@item.firstName</td>
                <td class="text-center">@item.lastName</td>
                <td class="text-center">@item.email</td>
                <td class="text-center">@item.phone</td>
            </tr>
        }
    </tbody>
</table>